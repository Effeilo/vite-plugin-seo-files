        "type": "git",
        "url": "https://github.com/Effeilo/vite-plugin-seo-files"
    },
    "bugs": {
        "url": "https://github.com/Effeilo/vite-plugin-seo-files/issues"
    },
    "homepage": "https://github.com/Effeilo/vite-plugin-seo-files",
    "peerDependencies": {
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "dependencies": {
        "glob": "^11.0.2"
    },
    "devDependencies": {
        "@types/node": "^24.0.7",
        "typescript": "^5.8.3",
        "vite": "^6.3.5"
